{
  "name": "@okikio/native",
  "version": "3.1.0",
  "type": "module",
  "umd": "native",
  "sideEffects": false,
  "description": "A framework to create complex, light-weight, and performant web applications using modern js apis.",
  "access": "public",
  "legacy": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "@types/index.d.ts",
  "browser": "lib/index.mjs",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "default": "./lib/index.mjs"
    },
    "./lib/*": "./lib/*.mjs",
    "./src/*": "./src/*.ts"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "@types": "./@types"
  },
  "files": [
    "lib",
    "src",
    "@types"
  ],
  "scripts": {
    "dts": "rm -r -f @types/ && tsc --project dts.tsconfig.json",
    "pre-release": "npm run dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/native.git"
  },
  "keywords": [
    "api",
    "ts",
    "native",
    "modern",
    "framework",
    "web",
    "pjax"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okikio/native/issues"
  },
  "homepage": "https://native.okikio.dev",
  "dependencies": {
    "path-to-regexp": "^6.2.1"
  },
  "peerDependencies": {
    "@okikio/emitter": "workspace:*",
    "@okikio/manager": "workspace:*"
  },
  "devDependencies": {
    "astro": "^1.5.2",
    "esno": "^0.16.3",
    "typescript": "^4.8.4",
    "ultra-runner": "^3.10.5",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}
