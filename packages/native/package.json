{
  "name": "@okikio/native",
  "version": "3.1.0",
  "type": "module",
  "umd": "native",
  "sideEffects": false,
  "description": "A framework to create complex, light-weight, and performant web applications using modern js apis.",
  "access": "public",
  "legacy": "lib/index.cjs",
  "main": "lib/index.cjs",
  "browser": "lib/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      },
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "build": "tsup",
    "test:run": "vitest run",
    "test": "vitest",
    "repl": "tsx repl.ts",
    "deno": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run ./repl.ts",
    "pre-release": "pnpm build; pnpm test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/native.git",
    "directory": "packages/native"
  },
  "keywords": [
    "api",
    "ts",
    "native",
    "modern",
    "framework",
    "web",
    "pjax"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okikio/native/issues"
  },
  "homepage": "https://native.okikio.dev",
  "dependencies": {
    "urlpattern-polyfill": "^10.0.0"
  },
  "peerDependencies": {
    "@okikio/emitter": "workspace:*",
    "@okikio/manager": "workspace:*"
  },
  "devDependencies": {
    "esbuild-plugin-umd-wrapper": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}
