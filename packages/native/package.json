{
    "name": "@okikio/native",
    "version": "3.1.0",
    "type": "module",
    "sideEffects": false,
    "description": "A framework who's goal is to make it easier to create complex, light-weight, and performant web applications using modern js apis.",
    "publishConfig": {
        "umd": "native",
        "access": "public",
        "legacy": "lib/api.js",
        "main": "lib/api.cjs",
        "types": "@types/api.d.ts",
        "browser": "lib/api.mjs",
        "module": "lib/api.mjs",
        "exports": {
            ".": {
                "require": "./lib/api.cjs",
                "import": "./lib/api.mjs",
                "default": "./lib/api.mjs"
            },
            "./lib/*": "./lib/*.mjs"
        }
    },
    "main": "src/api.ts",
    "directories": {
        "lib": "./lib",
        "src": "./src"
    },
    "files": [
        "lib",
        "src",
        "@types"
    ],
    "scripts": {
        "dts": "rm -r -f @types/ && tsc --project dts.tsconfig.json",
        "pre-release": "npm run dts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/okikio/native.git"
    },
    "keywords": [
        "api",
        "ts",
        "native",
        "modern",
        "framework",
        "web",
        "pjax"
    ],
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/okikio/native/issues"
    },
    "homepage": "https://github.com/okikio/native/tree/master/packages/native#readme",
    "dependencies": {
        "path-to-regexp": "^6.2.0"
    },
    "peerDependencies": {
        "@okikio/emitter": "workspace:*",
        "@okikio/manager": "workspace:*"
    },
    "devDependencies": {
        "typescript": "^4.4.2"
    },
    "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}