{
  "name": "@okikio/animate",
  "version": "2.4.0",
  "type": "module",
  "umd": "animate",
  "sideEffects": false,
  "description": "An animation library for the modern web which utilizes the Web Animation API (WAAPI) to create butter smooth animation.",
  "access": "public",
  "legacy": "lib/index.cjs",
  "main": "lib/index.cjs",
  "browser": "lib/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      },
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "build": "tsup",
    "test:run": "vitest run",
    "test": "vitest",
    "repl": "tsx repl.ts",
    "deno": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run ./repl.ts",
    "pre-release": "pnpm build; pnpm test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/native.git",
    "directory": "packages/animate"
  },
  "keywords": [
    "ts",
    "modern",
    "animation",
    "library",
    "web",
    "css",
    "smooth"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okikio/native/issues"
  },
  "homepage": "https://native.okikio.dev/animate",
  "peerDependencies": {
    "@okikio/emitter": "workspace:*",
    "@okikio/manager": "workspace:*"
  },
  "devDependencies": {
    "csstype": "^3.1.3",
    "esbuild-plugin-umd-wrapper": "^2.0.0",
    "jsr": "^0.12.4",
    "tsup": "^8.1.0",
    "tsx": "^4.13.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f",
  "dependencies": {
    "@dynimorius/color-utilities": "npm:@jsr/dynimorius__color-utilities@^1.0.11"
  }
}