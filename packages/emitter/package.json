{
    "name": "@okikio/emitter",
    "version": "2.1.8",
    "type": "module",
    "sideEffects": false,
    "description": "A small Event Emitter written in typescript with performance and ease of use in mind.",
    "publishConfig": {
        "umd": "emitter",
        "access": "public",
        "legacy": "lib/api.js",
        "main": "lib/api.cjs",
        "types": "@types/api.d.ts",
        "browser": "lib/api.mjs",
        "module": "lib/api.mjs",
        "exports": {
            ".": {
                "require": "./lib/api.cjs",
                "import": "./lib/api.mjs",
                "default": "./lib/api.mjs"
            },
            "./lib/*": "./lib/*.mjs"
        }
    },
    "main": "src/api.ts",
    "directories": {
        "lib": "./lib",
        "src": "./src",
        "@types": "./@types"
    },
    "files": [
        "lib",
        "src",
        "@types"
    ],
    "scripts": {
        "dts": "rm -r -f @types/ && tsc --project dts.tsconfig.json",
        "test": "jest",
        "pre-release": "npm test && npm run dts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/okikio/native.git"
    },
    "keywords": [
        "event",
        "emitter",
        "small",
        "light-weight",
        "ts",
        "typescript",
        "api"
    ],
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/okikio/native/issues"
    },
    "homepage": "https://github.com/okikio/native/tree/master/packages/emitter#readme",
    "peerDependencies": {
        "@okikio/manager": "workspace:*"
    },
    "devDependencies": {
        "esbuild": "^0.13.2",
        "esbuild-jest": "^0.5.0",
        "jest": "^27.2.1",
        "jest-chain": "^1.1.5",
        "jest-extended": "^0.11.5",
        "typescript": "^4.4.3"
    },
    "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}